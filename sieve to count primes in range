#count no.of primes in range
import math
n=100001
s=[1]*n
s[0]=0
s[1]=0
m=int(math.sqrt(n))
for i in range(2,n):
    if s[i]==1:
        for j in range(i*i,n,i):
            s[j]=0
c=0
n=int(input())
for i in range(n+1):
    start,end=map(int,input().split())
    for i in range(start,end+1):
        if s[i]==1:
            c=c+1
    print(c)
    c=0
