n=int(input())
m=[]
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    m.append(l)
for i in m:
    print(i)
for i in range(n):
    for j in range(n):
        if i==j :
            print('primary diagonal',m[i][j])
        if i+j==n-1:
            print('secondary diagonal',m[i][j])
print(max(m[i][j]))
