n=int(input())
A=[]
sum1=0
sum2=0
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    A.append(l)
for i in range(n):
    for j in range(n):
        if i==j:
            sum1=sum1+A[i][j]
print(sum1)
for i in range(n):
    for j in range(n):
        if i+j==n-1:
            sum2=sum2+A[i][j]
print(sum2)
print("difference",sum1-sum2)
